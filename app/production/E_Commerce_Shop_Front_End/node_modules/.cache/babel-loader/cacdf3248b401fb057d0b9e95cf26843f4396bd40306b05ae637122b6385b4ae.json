d\">\n            <div style=\"margin-top: 10px; font-weight: 500; font-size: 16px; width: 180px; color: #000000FF; text-overflow: ellipsis; overflow: hidden; white-space: nowrap;\">{{ recommendData[1]?.name }}</div>\n            <div style=\"margin-top: 5px; font-size: 20px; color: #FF5000FF\">￥{{ recommendData[1]?.price }}/{{ recommendData[1]?.unit }}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\nexport default {\n  data() {\n    let typeId = this.$route.query.id\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      typeId: typeId,\n      goodsData: [],\n      typeData: {},\n      recommendData:[]\n    }\n  },\n  mounted() {\n    this.loadGoods()\n    this.loadType()\n    this.loadRecommend()\n  },\n  computed: {\n    getImageSrc(index) {\n      return this.recommendData[index]?.img || require('@/assets/imgs/demo1.png'); // 使用 require 引入图片\n    }\n  },\n  methods: {\n    loadType() {\n      this.$request.get('/type/selectById/' + this.typeId).then(res => {\n        if (res.code === '200') {\n          this.typeData = res.data\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    loadGoods() {\n      this.$request.get('/goods/selectByTypeId?id=' + this.typeId).then(res => {\n        if (res.code === '200') {\n          this.goodsData = res.data\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    },\n    navTo(url) {\n      location.href = url\n    },\n    loadRecommend(){\n      this.$request.get('/goods/selectRecommend').then(res => {\n        if (res.code == '200') {\n          this.recommendData = res.data\n          console.log(this.recommendData)\n        } else {\n          this.$message.error(res.msg)\n        }\n      })\n    }\n  }\n}\n</script>"],"mappings":"AAoCA;EACAA,KAAA;IACA,IAAAC,MAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAR,MAAA,EAAA